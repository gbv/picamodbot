# Copy this file to /etc/apache2/sites-enabled/
# and sudo /etc/init.d/apache2 restart
<VirtualHost picamodbot.gbv.de:80>
    ProxyPreserveHost On
    ProxyRequests Off
    <Proxy *>
       Order Allow,Deny
       Allow from all
    </Proxy>
    ProxyPass / http://localhost:6024/
    ProxyPassReverse / http://localhost:6024/
    ProxyVia Off
</VirtualHost>
